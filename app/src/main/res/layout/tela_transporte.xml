<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="410dp"
    android:layout_height="730dp"
    android:background="@drawable/tela_transporte"
    tools:context=".telaTransporte">

    <EditText
        android:id="@+id/DataOT"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="80dp"
        android:inputType="date"
        android:hint="DIA/MES/ANO"
        android:background="@null"
        />

    <EditText
        android:id="@+id/MotoristaEdit"
        android:layout_width="192dp"
        android:layout_height="41dp"
        android:hint="Motorista"
        android:layout_marginStart="107dp"
        android:layout_marginTop="160dp"
        android:background="@null"
        />

    <EditText
        android:id="@+id/veiculoEdit"
        android:layout_width="192dp"
        android:layout_height="41dp"
        android:layout_marginStart="107dp"
        android:layout_marginTop="224dp"
        android:background="@null"
        android:hint="Veiculo" />

    <EditText
        android:id="@+id/placaEdit"
        android:layout_width="192dp"
        android:layout_height="41dp"
        android:hint="Placa"
        android:layout_marginStart="107dp"
        android:layout_marginTop="287dp"
        android:background="@null"
        />

    <EditText
        android:id="@+id/KmSaidaEdit"
        android:layout_width="192dp"
        android:layout_height="41dp"
        android:hint="Km saida"
        android:layout_marginStart="107dp"
        android:layout_marginTop="348dp"
        android:inputType="number"
        android:background="@null"
        />

    <EditText
        android:id="@+id/KmFinalEdit"
        android:layout_width="192dp"
        android:layout_height="41dp"
        android:hint="Km Final"
        android:layout_marginStart="107dp"
        android:layout_marginTop="413dp"
        android:inputType="number"
        android:background="@null"
        />

    <EditText
        android:id="@+id/HoraInicialEdit"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:hint="Hora Inicial"
        android:layout_marginStart="39dp"
        android:layout_marginTop="494dp"
        android:inputType="datetime"
        android:background="@null"
        />

    <EditText
        android:id="@+id/DataHI"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:hint="Dia/Mes/Ano"
        android:layout_marginStart="39dp"
        android:layout_marginTop="566dp"
        android:inputType="date"
        android:background="@null"
        />

    <EditText
        android:id="@+id/HoraFinalEdit"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:hint="Hora Final"
        android:layout_marginStart="244dp"
        android:layout_marginTop="495dp"
        android:inputType="datetime"
        android:background="@null"
        />

    <EditText
        android:id="@+id/DataHF"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:hint="Dia/Mes/Ano"
        android:layout_marginStart="244dp"
        android:layout_marginTop="566dp"
        android:inputType="date"
        android:background="@null"
        />

    <EditText
        android:id="@+id/PernoitesEdit"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:hint="Pernoites"
        android:layout_marginStart="138dp"
        android:layout_marginTop="619dp"
        android:inputType="number"
        android:background="@null"
        />

    <ImageView
        android:id="@+id/bs_image"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:src="@drawable/bs"
        android:layout_marginStart="80dp"
        android:layout_marginTop="660dp"
        />

    <ImageButton
        android:id="@+id/salvar_bot達o"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="680dp"
        android:background="@null"
        android:contentDescription="Bot達o de salvar"
        />

    <ImageView
        android:id="@+id/benviar_image"
        android:layout_width="120dp"
        android:layout_height="80dp"
        android:src="@drawable/benviar"
        android:layout_marginStart="220dp"
        android:layout_marginTop="660dp"
        />
    <ImageButton
        android:id="@+id/enviar_bot達o"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="680dp"
        android:background="@null"
        android:contentDescription="Bot達o de envio"
        />
</RelativeLayout>